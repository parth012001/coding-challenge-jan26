# SurrealDB Cloud Configuration
SURREAL_URL=wss://your-instance.surreal.cloud/rpc
SURREAL_NAMESPACE=production
SURREAL_DATABASE=matchmaking
SURREAL_USERNAME=your_username
SURREAL_PASSWORD=your_password

# SurrealDB HTTP endpoint (used by frontend dashboard metrics at build time)
SURREAL_HTTP_URL=https://your-instance.surreal.cloud

# OpenAI (used by edge functions for LLM-powered match explanations)
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_MODEL=gpt-4o-mini

# Supabase Edge Functions (used by frontend to call backend)
NEXT_PUBLIC_SUPABASE_FUNCTIONS_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
